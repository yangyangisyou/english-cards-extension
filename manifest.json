{
    "name": "翻譯單字卡",
    "version": "1.0",
    "description": "就是翻譯",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [{
        "matches": ["https://*/*", "https://*/*"],
        "js": ["contentScript.js"]
    }],
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "review.html"
  }